{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:8080"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApplicationDatabase": "Server=<server>; Database=<database>; User Id=<userId>; Password=<password>; Encrypt=false;"
  },
  "JwtSettings": {
    "SecretKey": "JWT-SECRET-KEY-32-CHARACTER-LONG",
    "Issuer": "domain-name",
    "Audience": "domain-name",
    "ExpiresInMinutes": 60
  },
  "AesSettings": {
    "EncryptionKey": "AES-256-KEY-32-CHARACTER-LONG",
    "IV": "AES-IV-16-CHARACTER-LONG"
  },
  "Redis": {
    "ConnectionString": "IP-ADDRESS:PORT"
  },
  "RateLimitSettings": {
    "MaxAttempts": 6,
    "LockoutDurationMinutes": 15
  }
}
